# Database
DATABASE_URL=postgresql://user:password@host:5432/database

# JWT Configuration
JWT_SECRET=your-256-bit-secret-key-here
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=another-256-bit-secret-key-here
JWT_REFRESH_EXPIRES_IN=30d

# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5000

# Cloudflare R2 Storage
R2_ACCOUNT_ID=your-account-id
R2_ACCESS_KEY_ID=your-access-key-id
R2_SECRET_ACCESS_KEY=your-secret-access-key
R2_BUCKET_NAME=your-bucket-name
R2_PUBLIC_URL=https://your-r2-public-url.com

# BeSign API
BESIGN_API_URL=https://app-sign.efcaz.com.br/efcaz-clm/api
BESIGN_API_KEY=your-besign-api-key

# PlugSign API
PLUGSIGN_API_URL=https://app.plugsign.com.br
PLUGSIGN_API_KEY=your-plugsign-api-key

# Webhook Secret (for signature webhooks)
WEBHOOK_SECRET=your-webhook-secret

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000
